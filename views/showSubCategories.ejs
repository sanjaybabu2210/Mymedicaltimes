<%- include('partials/header.ejs'); -%>


<div class="col-lg-7 col-md-7 col-sm-9" id="main">
 
  <% art.forEach(function(art){ %>
  <div class="list-item "
    style="min-height:200px;border-radius:5px;border:1px solid white; background-color: white;margin-top:15px;padding-left: 3%"
    class="hov11">
    <a href="/<%= art.mainHeading %>/<%= art.subHeading %>/<%= art._id %>" target="_blank"
      style="font-size:1.1rem;margin-top: 5%; text-decoration: none;color: primary;text-align:left;">
      <strong style="text-align: left;"><%= art.title %></strong></a><br>
    <%  var today = art.createdAt;var year = today.getFullYear();
  var mes = today.getMonth()+1;
  var dia = today.getDate();
  var fecha =dia+"-"+mes+"-"+year; %>
    <h4 style="font-size:12px;color: grey;margin-bottom:2%;position: relative;text-align: left;"><strong>last
        updated:<%= fecha %></strong>
    </h4>
    <p style="font-size: 16px; color: rgba(0,0,0,0.7);margin-bottom:2%;"><%= art.shortDescription %>...<a
        href="/<%= art.mainHeading %>/<%= art.subHeading %>/<%= art._id %>" class="hov">Read More</a></p>
    <h4 style="font-size:14px;color: primary;margin-bottom:2%;position: relative;"><strong>By : <%= art.author%></strong>
    </h4>
  </div>
  
  <% }) %>
	
</div>
</div>

<script type="text/javascript">
  
  // jQuery Plugin: http://flaviusmatis.github.io/simplePagination.js/

var items = $(".list-wrapper .list-item");
    var numItems = items.length;
    var perPage = 14;

    items.slice(perPage).hide();

    $('#pagination-container').pagination({
        items: numItems,
        itemsOnPage: perPage,
        prevText: "&laquo;",
        nextText: "&raquo;",
        onPageClick: function (pageNumber) {
            var showFrom = perPage * (pageNumber - 1);
            var showTo = showFrom + perPage;
            items.hide().slice(showFrom, showTo).show();
        }
    });
  
</script>




