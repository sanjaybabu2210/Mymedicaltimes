  <% if(error.length>0){%>
<div class="alert alert-danger container" style="margin-top:0px;"><%=error%></div>
<% }if(success.length>0){%>
<div class="alert alert-success container" style="margin-top:0px;"><%=success%></div>
<% } %> 

<h2 style="text-align:center;margin-top:2%"><b>Applied Intern Details</b></h2>

<table style="margin:5%;box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);margin-top:2%;margin-bottom:15%">
  <tr>
    <th>Email</th>
    <th>Full Name</th>
    <th>College</th>
    <th>Question</th>
  </tr>
   <% use.forEach(function(u){ %>
  <tr>
    <td><%= u.email %> </td>
    <td><%= u.name %> <button class="btn btn-info btn-sm">See CV</button></td>
    <td><%= u.college %>  </td>
    <td><%= u.question %> </td>
  </tr>
  <% }) %>
  
</table>



