body{
    background-color: #ffe9ec
}
.sidebar{
	position: scroll;
	width: 350px;
	left: 0;
	background: #1b1b1b;
	height: 100%;
	margin: 0px;


}
.sidebar .text{
	color: white;
	font-size: 25px;
	font-weight: 600;
	line-height: 65px;
	text-align: center;
	background: #1e1e1e;
	letter-spacing: 1px;
	margin: 0px
}
 nav ul{
	background : #1b1b1b;
	height: 100%;
	width: 100%;
	list-style: none;
	
} 
nav ul li{
	line-height: 60px;
	border-bottom: 1px solid rgba(255, 255, 255, 0.1);

}

nav ul li a{
	position: relative;
	color: white;
	text-decoration: none;
	font-size: 18px;
	padding-left: 40px;
	font-weight: 500;
	display: block;
	width: 100%;
	border-left: 3px solid transparent;
}
nav ul li.active a{
	color:cyan;
	background: #1e1e1e;
	border-left-color: cyan;
	
}
nav ul ul{
	position: static;
	display:none;
}
nav ul .feat-show.show{
	display: block;
}
nav ul .an-show.show2{
	display: block;
}
nav ul .ant-show.show3{
	display: block;
}
nav ul .bi-show.show4{
	display: block;
}
nav ul .bio-show.show5{
	display: block;
}
nav ul .bip-show.show6{
	display: block;
}
nav ul .co-show.show7{
	display: block;
}
nav ul .den-show.show8{
	display: block;
}
nav ul .med-show.show9{
	display: block;
}
nav ul .mib-show.show10{
	display: block;
}
nav ul .mir-show.show11{
	display: block;
}
nav ul .oc-show.show12{
	display: block;
}
nav ul .pat-show.show13{
	display: block;
}
nav ul .pha-show.show14{
	display: block;
}
nav ul .phm-show.show15{
	display: block;
}
nav ul .phs-show.show16{
	display: block;
}
nav ul .phsi-show.show17{
	display: block;
}
nav ul .psy-show.show18{
	display: block;
}
nav ul .sur-show.show19{
	display: block;
}
nav ul .the-show.show20{
	display: block;
}
nav ul .yg-show.show21{
	display: block;
}
nav ul .ve-show.show22{
	display: block;
}
nav ul ul li{
	line-height: 42px;
	border-bottom: none;
}
nav ul ul li a{
	font-size: 17px;
	color: #e6e6e6;
	padding-left: 80px;
}
nav ul li.active ul li a{
	color:#e6e6e6;
	background: #1b1b1b;
	border-left-color: transparent;
	
}
nav ul ul li a:hover{
	color:cyan!important;
	background:1e1e1e!important;
}
nav ul li a span{
	position: absolute;
	top: 50%;
	right: 20px;
	transform: translateY(-50%);
	font-size: 22px;
	transition: transform 0.4s;
}
nav ul li a span.rotate{
	transform:translateY(-50%) rotate(-180deg);
}
footer{
	background: #1c2331
}
.dropdown-submenu{
    position: relative;
}
.dropdown-submenu a::after{
    transform: rotate(-90deg);
    position: relative;
    right: 3px;
    top: 40%;
}
footer{
	background: #1c2331;
  position: absolute;
  width: 100%;
  left: 0;
  bottom: -70%;
}
.dropdown-submenu:hover .dropdown-menu, .dropdown-submenu:focus .dropdown-menu{
    display: flex;
    flex-direction: column;
    position: scroll !important;
    margin-top: -30px;
    left: 100%;
}
@media (max-width: 992px) {
    .dropdown-menu{
        width: 50%;
    }
    .dropdown-menu .dropdown-submenu{
        width: auto;
    }
}


